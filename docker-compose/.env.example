# AI Infrastructure Stack - Environment Variables
# Copy this file to .env and configure with your actual values

# Domain Configuration
N8N_HOST=n8n.example.com
N8N_MCP_BASE_URL=https://n8n-mcp.example.com
N8N_API_URL=https://n8n.example.com

# Obsidian Configuration
OBSIDIAN_USER=obsidian
OBSIDIAN_PASSWORD=your-secure-password

# API Keys and Tokens
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here
LEANN_API_TOKEN=your-leann-api-token

# n8n Configuration
N8N_API_KEY=your-n8n-api-key-jwt-token

# n8n MCP Configuration  
N8N_MCP_AUTH_TOKEN=your-n8n-mcp-auth-token

# Additional Configuration
TZ=UTC
NODE_ENV=production